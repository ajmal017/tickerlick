select distinct d.ticker from (select 'RSI' as indicator,'Buy' as tradesignal ,d.ticker,d.rtq,d.rsi_14,d.sma_3_3_stc_osci_14 from secpricert d,(select a.ticker,max(a.seq) maxseq from secpricert a where a.price_date = '2018-05-25' group by a.ticker) b where d.seq = b.maxseq and d.ticker = b.ticker  and d.rsi_14 < 25  and d.rsi_14 != 0 union select distinct d.ticker from secpricert d,(select a.ticker,max(a.seq) maxseq from secpricert a where a.price_date = '2018-05-25' group by a.ticker) b where d.seq = b.maxseq and d.ticker = b.ticker  and d.rsi_14 > 75  and d.rsi_14 != 0 union select distinct d.ticker from secpricert d,(select a.ticker,max(a.seq) maxseq from secpricert a where a.price_date = '2018-05-25' group by a.ticker) b where d.seq = b.maxseq and d.ticker = b.ticker  and d.sma_3_3_stc_osci_14 < 20 and d.sma_3_3_stc_osci_14  >  1 union select distinct d.ticker from secpricert d,(select a.ticker,max(a.seq) maxseq from secpricert a where a.price_date = '2018-05-25' group by a.ticker) b where d.seq = b.maxseq and d.ticker = b.ticker  and d.sma_3_3_stc_osci_14 > 80 and d.sma_3_3_stc_osci_14 < 99 union select distinct d.ticker from secpricert d,(select a.ticker,max(a.seq) maxseq from secpricert a where a.price_date = '2018-05-25' group by a.ticker) b where d.seq = b.maxseq and d.ticker = b.ticker and d.rtq < (d.dma_20 - (2 * d.dma_20_sd)) union select distinct d.ticker from secpricert d,(select a.ticker,max(a.seq) maxseq from secpricert a where a.price_date = '2018-05-25' group by a.ticker) b where d.seq = b.maxseq and d.ticker = b.ticker and d.rtq > abs((d.dma_20 + (2 * d.dma_20_sd)))) x;
